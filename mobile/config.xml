<?xml version='1.0' encoding='utf-8'?>
<widget id="com.resolveme.io" version="0.14.1" ios-CFBundleVersion="14" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Resolve Me</name>
    <description>
        The powerfull calculator for all your needs
    </description>
    <author email="support@resolveme.io" href="https://resolveme.io">
       MSV84366NV
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <icon src="res/Icon-16.png" width="16" />
    <icon src="res/Icon-20.png" width="20" />
    <icon src="res/Icon-29.png" width="29" />
    <icon src="res/Icon-32.png" width="32" />
    <icon src="res/Icon-40.png" width="40" />
    <icon src="res/Icon-48.png" width="48" />
    <icon src="res/Icon-50.png" width="50" />
    <icon src="res/Icon-57.png" width="57" />
    <icon src="res/Icon-58.png" width="58" />
    <icon src="res/Icon-60.png" width="60" />
    <icon src="res/Icon-64.png" width="64" />
    <icon src="res/Icon-72.png" width="72" />
    <icon src="res/Icon-76.png" width="76" />
    <icon src="res/Icon-80.png" width="80" />
    <icon src="res/Icon-87.png" width="87" />
    <icon src="res/Icon-88.png" width="88" />
    <icon src="res/Icon-100.png" width="100" />
    <icon src="res/Icon-114.png" width="114" />
    <icon src="res/Icon-120.png" width="120" />
    <icon src="res/Icon-128.png" width="128" />
    <icon src="res/Icon-144.png" width="144" />
    <icon src="res/Icon-152.png" width="152" />
    <icon src="res/Icon-167.png" width="167" />
    <icon src="res/Icon-172.png" width="172" />
    <icon src="res/Icon-180.png" width="180" />
    <icon src="res/Icon-196.png" width="196" />
    <icon src="res/Icon-256.png" width="256" />
    <icon src="res/Icon-512.png" width="512" />
    <icon src="res/Icon-1024.png" width="1024" />
     
    <platform name="android">
        <preference name="StatusBarStyle" value="lightcontent" />
        <allow-intent href="market:*" />
    </platform>

    <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />
      <preference name="FadeSplashScreenDuration" value="500"/>
      <preference name="WKWebViewOnly" value="true" />
      <preference name="UIWebViewBounce" value="false" />
      <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
      <preference name="deployment-target" value="12.0" />
      <feature name="CDVWKWebViewEngine">
          <param name="ios-package" value="CDVWKWebViewEngine" />
      </feature>
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-clipboard" spec="~1.3.0" />
</widget>
